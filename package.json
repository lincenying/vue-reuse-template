{
  "name": "@lincy/vue-reuse-template",
  "type": "module",
  "version": "1.0.1",
  "packageManager": "pnpm@8.1.1",
  "description": "Define and reuse Vue template inside the component scope.",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/vue-reuse-template#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/vue-reuse-template.git"
  },
  "bugs": "https://github.com/antfu/vue-reuse-template/issues",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "play": "nr -C playground dev",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish --access public",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "@antfu/ni": "^0.21.6",
    "@antfu/utils": "^0.7.6",
    "@types/node": "^20.5.7",
    "@vitejs/plugin-vue": "^4.3.3",
    "@vue/test-utils": "^2.4.1",
    "bumpp": "^9.2.0",
    "eslint": "^8.48.0",
    "esno": "^0.17.0",
    "happy-dom": "^10.11.0",
    "lint-staged": "^14.0.1",
    "pnpm": "^8.7.0",
    "rimraf": "^5.0.1",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vite": "^4.4.9",
    "vitest": "^0.34.3",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.8"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
